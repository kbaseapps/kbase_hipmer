{
    "authors": [
        "scanon",
	    "dylan",
		"jfroula",
        "seanjungbluth"
    ],
    "behavior": {
        "service-mapping": {
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "usedebug",
                    "target_property": "usedebug"
                },
                {
                    "input_parameter": "output_contigset_name",
                    "target_property": "output_contigset_name"
                },
                {
                    "input_parameter": "mer_sizes",
                    "target_property": "mer_sizes"
                },
                {
                    "input_parameter": "reads",
                    "target_property": "reads"
                },
                {
                    "input_parameter": "ins_avg",
                    "target_property": "ins_avg"
                },
                {
                    "input_parameter": "ins_dev",
                    "target_property": "ins_dev"
                },
                {
                    "constant_value": 1,
                    "target_property": "is_plant"
                },
                {
                    "input_parameter": "diploid",
                    "target_property": "diploid"
                },
                {
                    "input_parameter": "assembly_size_filter",
                    "target_property": "assembly_size_filter"
                }
            ],
            "method": "run_hipmer_hpc",
            "name": "hipmer",
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "service_method_output_path": [
                        0,
                        "report_name"
                    ],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [
                        0,
                        "report_ref"
                    ],
                    "target_property": "report_ref"
                },
                {
                    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ],
            "url": ""
        }
    },
    "categories": [
        "active",
        "assembly"
    ],
    "contact": "http://kbase.us/contact-us/",
    "job_id_output_field": "docker",
    "parameter-groups": [
        {
            "id": "reads",
            "allow_multiple": true,
            "optional": false,
            "parameters": [
                "read_library_name",
                "ins_avg",
                "ins_dev"
            ],
            "with_border": true
        }
    ],
    "parameters": [
        {
            "id": "output_contigset_name",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "hipmer.contigs"
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "is_output_name": true,
                "valid_ws_types": [
                    "KBaseGenomes.ContigSet"
                ]
            }
        },
        {
            "id": "mer_sizes",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "21,41,63,95,127"
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "validate_as": "text"
            }
        },
        {
            "id": "ins_avg",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                ""
            ],
            "field_type": "text",
            "optional": true,
            "text_options": {
                "min_int": 1,
                "validate_as": "int"
            }
        },
        {
            "id": "ins_dev",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                ""
            ],
            "field_type": "text",
            "optional": true,
            "text_options": {
                "min_int": 1,
                "validate_as": "int"
            }
        },
        {
            "id": "read_library_name",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                ""
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "valid_ws_types": [
                    "KBaseAssembly.PairedEndLibrary",
                    "KBaseFile.PairedEndLibrary"
                ]
            }
        },
        {
            "id" : "diploid",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "none" ],
            "field_type" : "dropdown",
            "dropdown_options":{
              "options": [
                {
                  "value": "none",
                  "display": "haploid"
                },
                {
                  "value": "low",
                  "display": "low heterzygosity"
                },
                {
                  "value": "high",
                  "display": "high heterzygosity"
                }
              ]
            }
        },
        {
            "id": "assembly_size_filter",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "2000"
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "min_int": 1,
                "validate_as": "int"
            }
        },
        {
            "id": "usedebug",
            "advanced": true,
            "allow_multiple": false,
            "checkbox_options": {
                "checked_value": 1,
                "unchecked_value": 0
            },
            "default_values": [
                "0"
            ],
            "field_type": "checkbox",
            "optional": false
        }
    ],
    "ver": "1.2.2-7",
    "visible": true,
    "widgets": {
        "input": null,
        "output": "no-display"
    }
}
