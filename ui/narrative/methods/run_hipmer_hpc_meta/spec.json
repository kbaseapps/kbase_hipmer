    "parameters": [
        {
            "id": "output_contigset_name",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "hipmer.contigs"
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "is_output_name": true,
                "valid_ws_types": [
                    "KBaseGenomes.ContigSet"
                ]
            }
        },
        {
            "id": "mer_sizes",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "21,33,55,77,99"
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "validate_as": "text"
            }
        },
        {
            "id": "scaff_mer_sizes",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "99,33"
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "validate_as": "text"
            }
        },
        {
            "id": "reads",
            "advanced": false,
            "allow_multiple": true,
            "default_values": [
                ""
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "valid_ws_types": [
                    "KBaseAssembly.PairedEndLibrary",
                    "KBaseFile.PairedEndLibrary"
                ]
            }
        },
        {
            "id": "assembly_size_filter",
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "2000"
            ],
            "field_type": "text",
            "optional": false,
            "text_options": {
                "min_int": 1,
                "validate_as": "int"
            }
        },
        {
            "id": "usedebug",
            "advanced": true,
            "allow_multiple": false,
            "checkbox_options": {
                "checked_value": 1,
                "unchecked_value": 0
            },
            "default_values": [
                "0"
            ],
            "field_type": "checkbox",
            "optional": false
        }
    ],
    "ver": "2.1.0-1",
    "visible": true,
    "widgets": {
        "input": null,
        "output": "no-display"
    }
